<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        package="com.example.android.sample.testapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme"
            tools:ignore="GoogleAppIndexingWarning">

        <activity android:name=".MainActivity"
                android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".InterviewListActivity"
                android:screenOrientation="portrait" />
        <activity android:name=".CollegeListActivity"
                android:screenOrientation="portrait" />
        <activity android:name=".FacilityDetaileActivity"
                android:screenOrientation="portrait" />
        <activity android:name=".FacilityListActivity"
                android:screenOrientation="portrait" />
        <activity android:name=".StampActivity"
                android:screenOrientation="portrait" />
        <activity android:name=".BustimeActivity"
                android:screenOrientation="portrait" />
        <activity android:name=".MapActivity"
                android:screenOrientation="portrait" />
        <activity android:name=".InterviewActivity"
                android:screenOrientation="portrait" />
        <activity android:name=".DocumentActivity"
                android:screenOrientation="portrait" />
        <activity android:name=".SubjectActivity"
                android:screenOrientation="portrait" />
        <activity android:name=".CalenderActivity"
                android:screenOrientation="portrait" />
        <activity android:name=".CampusInfoActivity"
                android:screenOrientation="portrait" />
        <activity android:name=".ToolbarActivity"
                android:screenOrientation="portrait" />
    </application>

</manifest>