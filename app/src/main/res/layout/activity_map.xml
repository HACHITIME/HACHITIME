    <ImageView
            android:id="@+id/MapImage"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:scrollbars="horizontal|vertical"
            app:layout_constraintBottom_toTopOf="@+id/linearLayout"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView2"
            app:srcCompat="@drawable/map_image" />
    <ImageView
            android:id="@+id/imageView"
            android:layout_width="85dp"
            android:layout_height="53dp"
            app:layout_constraintBottom_toBottomOf="@+id/MapImage"
            app:layout_constraintEnd_toStartOf="@+id/MapImage"
            app:layout_constraintHorizontal_bias="0.34"
            app:layout_constraintStart_toEndOf="@id/MapImage"
            app:layout_constraintTop_toTopOf="@+id/MapImage"
            app:layout_constraintVertical_bias="0.33999997"
            app:srcCompat="@drawable/fukidasi_tecno" />
    <ImageView
            android:id="@+id/imageView2"
            android:layout_width="85dp"
            android:layout_height="53dp"
            app:layout_constraintBottom_toTopOf="@+id/MapImage"
            app:layout_constraintEnd_toStartOf="@+id/MapImage"
            app:layout_constraintHorizontal_bias="0.453"
            app:layout_constraintStart_toEndOf="@+id/MapImage"
            app:layout_constraintTop_toBottomOf="@+id/MapImage"
            app:layout_constraintVertical_bias="0.68"
            app:srcCompat="@drawable/fukidasi_desain" />
    <ImageView
            android:id="@+id/imageView3"
            android:layout_width="85dp"
            android:layout_height="53dp"
            app:layout_constraintBottom_toTopOf="@+id/MapImage"
            app:layout_constraintEnd_toStartOf="@+id/MapImage"
            app:layout_constraintHorizontal_bias="0.62"
            app:layout_constraintStart_toEndOf="@+id/MapImage"
            app:layout_constraintTop_toBottomOf="@+id/MapImage"
            app:layout_constraintVertical_bias="0.73"
            app:srcCompat="@drawable/fukidasi_it" />
    <ImageView
            android:id="@+id/imageView4"
            android:layout_width="85dp"
            android:layout_height="53dp"
            app:layout_constraintBottom_toTopOf="@+id/MapImage"
            app:layout_constraintEnd_toStartOf="@+id/MapImage"
            app:layout_constraintHorizontal_bias="0.76"
            app:layout_constraintStart_toEndOf="@+id/MapImage"
            app:layout_constraintTop_toBottomOf="@+id/MapImage"
            app:layout_constraintVertical_bias="0.82"
            app:srcCompat="@drawable/fukidasisports" />
    <ImageView
            android:id="@+id/imageView5"
            android:layout_width="85dp"
            android:layout_height="53dp"
            app:layout_constraintBottom_toTopOf="@+id/MapImage"
            app:layout_constraintEnd_toStartOf="@+id/MapImage"
            app:layout_constraintHorizontal_bias="0.32"
            app:layout_constraintStart_toEndOf="@+id/MapImage"
            app:layout_constraintTop_toBottomOf="@+id/MapImage"
            app:layout_constraintVertical_bias="0.74"
            app:srcCompat="@drawable/fukidasi_music" />
    <ImageView
            android:id="@+id/imageView6"
            android:layout_width="85dp"
            android:layout_height="53dp"
            app:layout_constraintBottom_toTopOf="@+id/MapImage"
            app:layout_constraintEnd_toStartOf="@+id/MapImage"
            app:layout_constraintHorizontal_bias="0.52"
            app:layout_constraintStart_toEndOf="@+id/MapImage"
            app:layout_constraintTop_toBottomOf="@+id/MapImage"
            app:layout_constraintVertical_bias="0.82"
            app:srcCompat="@drawable/fukidasi_iryou" />
    <ImageView
            android:id="@+id/imageView7"
            android:layout_width="85dp"
            android:layout_height="53dp"
            app:layout_constraintBottom_toTopOf="@+id/MapImage"
            app:layout_constraintEnd_toStartOf="@+id/MapImage"
            app:layout_constraintHorizontal_bias="0.51"
            app:layout_constraintStart_toEndOf="@+id/MapImage"
            app:layout_constraintTop_toBottomOf="@+id/MapImage"
            app:layout_constraintVertical_bias="0.44"
            app:srcCompat="@drawable/fukidasi_desain" />
    <android.support.v7.widget.Toolbar
            android:id="@+id/toolbar"
            android:layout_width="0dp"
            android:layout_height="60dp"
            android:background="@drawable/toolbar_style"
            android:minHeight="?attr/actionBarSize"
            android:theme="?attr/actionBarTheme"
            app:layout_constraintBottom_toTopOf="@+id/view"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0" />
    <LinearLayout
            android:id="@+id/linearLayout"
            android:layout_width="match_parent"
            android:layout_height="85dp"
            android:background="#ffffff"
            android:orientation="horizontal"
            android:padding="10dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0">
        <ImageButton
                android:id="@+id/school_btn"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@drawable/toolbar_flame_style"
                android:contentDescription="@string/school_btn"
                app:srcCompat="@drawable/toolbar_school" />
        <ImageButton
                android:id="@+id/map_btn"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="2dp"
                android:layout_weight="1"
                android:background="@drawable/toolbar_flame_style"
                android:contentDescription="@string/school_btn"
                app:srcCompat="@drawable/toolbar_map" />
        <ImageButton
                android:id="@+id/stamp_btn"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="2dp"
                android:layout_weight="1"
                android:background="@drawable/toolbar_flame_style"
                android:contentDescription="@string/school_btn"
                app:srcCompat="@drawable/toolbar_stamp" />
        <ImageButton
                android:id="@+id/info_btn"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="2dp"
                android:layout_weight="1"
                android:background="@drawable/toolbar_flame_style"
                android:contentDescription="@string/school_btn"
                app:srcCompat="@drawable/toolbar_info" />
        <ImageButton
                android:id="@+id/bus_btn"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="2dp"
                android:layout_weight="1"
                android:background="@drawable/toolbar_flame_style"
                android:contentDescription="@string/school_btn"
                app:srcCompat="@drawable/toolbar_bus" />
    </LinearLayout>
    <ImageButton
            android:id="@+id/modol_btn"
            android:layout_width="41dp"
            android:layout_height="32dp"
            android:background="@drawable/toolbar_modoru_style"
            android:contentDescription="@string/modol_btn"
            app:layout_constraintBottom_toBottomOf="@+id/toolbar"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.076"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.785"
            app:srcCompat="@drawable/toolbar_modoru" />
    <TextView
            android:id="@+id/tytle"
            android:layout_width="195dp"
            android:layout_height="42dp"
            android:layout_gravity="center"
            android:color="#000000"
            android:gravity="center"
            android:textColor="#ffffff"
            android:textSize="36sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.025" />
    <View
            android:id="@+id/view"
            android:layout_width="fill_parent"
            android:layout_height="4dp"
            android:layout_marginTop="456dp"
            android:background="@android:color/darker_gray"
            app:layout_constraintBottom_toTopOf="@+id/linearLayout"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0" />
    <TextView
            android:id="@+id/textView2"
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:background="@drawable/atonanko_style"
            android:fontFamily="sans-serif"
            android:gravity="center"
            android:text="あと7個 手に入れよう！"
            android:textColor="#FFFFFF"
            android:textSize="30dp"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/MapImage"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/toolbar" />

    <Button
            android:id="@+id/button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="図書館棟取得"
            app:layout_constraintBottom_toTopOf="@+id/view"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView7" />
</android.support.constraint.ConstraintLayout>